#: theme/templates/helping_center.html:802 theme/templates/legal.html:190
msgid "Modification of the general conditions of use"
msgstr "Modification des conditions générales d'utilisation"

#: theme/templates/helping_center.html:805 theme/templates/legal.html:193
msgid ""
